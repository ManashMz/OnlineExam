import random
f=(1,2,3)
g=random.choice(f)
print("The 1st value  is",g)

h=(1,2,3)
i=random.choice(h)
print("The 2nd value  is",i)

j=(1,2,3)
k=random.choice(j)
print("The 3rd value is",k)

if(g==i or i==k or k==g):
    print("You are the winner")
else :
    print("Better luck next time !")